{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:keyvault": "VITE_USE_KEY_VAULT=true vite --host",
    "dev:env": "VITE_USE_KEY_VAULT=false vite --host",
    "build": "vite build",
    "build:production": "bash scripts/build-production.sh",
    "build:protected": "vite build --config vite.config.production.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:rules": "jest --config jest.config.rules-engine.js",
    "test:rules:unit": "jest --config jest.config.rules-engine.js --selectProjects='Unit Tests'",
    "test:rules:integration": "jest --config jest.config.rules-engine.js --selectProjects='Integration Tests'",
    "test:rules:performance": "jest --config jest.config.rules-engine.js --selectProjects='Performance Tests'",
    "test:rules:watch": "jest --config jest.config.rules-engine.js --watch",
    "test:rules:coverage": "jest --config jest.config.rules-engine.js --coverage",
    "test:coverage:report": "open coverage/rules-engine/index.html",
    "setup:keyvault": "node scripts/setup-keyvault.js",
    "import-skills": "node scripts/import-skills.mjs",
    "import-skills:ts": "tsx scripts/import-skills.ts",
    "import-skills:dry-run": "npm run import-skills -- --dry-run --verbose",
    "import-skills:all": "npm run import-skills -- --all-prek-k --verbose",
    "validate-excel": "node scripts/validate-excel.mjs",
    "db:setup": "node scripts/database-setup.mjs setup",
    "db:seed": "node scripts/database-setup.mjs seed",
    "db:migrate": "node scripts/run-all-migrations.mjs",
    "import:common-core": "node database/scripts/importCommonCoreStandards.js",
    "import:grade10-skills": "node scripts/import-grade10-skills.mjs",
    "db:reset": "node scripts/database-setup.mjs reset",
    "db:check": "node scripts/database-setup.mjs check",
    "db:import-prek": "node scripts/database-setup.mjs import-prek",
    "db:import-k": "node scripts/database-setup.mjs import-k",
    "db:import-all-early": "node scripts/database-setup.mjs import-all-early",
    "db:seed-large": "node scripts/database-setup.mjs seed --student-count 20 --skills-per-subject 50",
    "db:full-setup": "npm run db:setup && npm run db:import-all-early && npm run db:seed",
    "profiles:migrate": "node scripts/run-profile-migration.mjs migrate",
    "profiles:seed": "node scripts/run-profile-migration.mjs profiles",
    "profiles:setup": "node scripts/run-profile-migration.mjs full",
    "profiles:verify": "node scripts/verify-profiles.mjs",
    "testbed:generate": "node scripts/generate-testbed.js generate",
    "testbed:small": "node scripts/generate-testbed.js generate --start-grade=Pre-K --end-grade=K --subjects=Math,Science",
    "testbed:full": "node scripts/generate-testbed.js generate --full-testbed",
    "testbed:math": "node scripts/generate-testbed.js generate --subjects=Math --start-grade=Pre-K --end-grade=2",
    "testbed:plainview": "node scripts/plainview-isd-testbed.js",
    "db:fix-grades": "npx supabase db push",
    "test:taylor": "tsx src/scripts/runTaylorTests.ts",
    "test:question-types": "tsx src/scripts/runTaylorTests.ts",
    "test:taylor:execute": "tsx src/scripts/executeTaylorTests.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@azure/communication-email": "^1.0.0",
    "@azure/identity": "^4.10.2",
    "@azure/keyvault-secrets": "^4.10.0",
    "@sentry/react": "^10.6.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.39.7",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.0.10",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.0.1",
    "lucide-react": "^0.344.0",
    "openai": "^5.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-obfuscator": "^1.1.0",
    "supabase": "^1.148.6",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-compression": "^0.5.1"
  }
}
